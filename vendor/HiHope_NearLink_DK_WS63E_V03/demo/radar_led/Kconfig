config SAMPLE_SUPPORT_RADAR_STA
    bool
    prompt "Support RADAR_STA Sample."
    default n
    depends on SAMPLE_SUPPORT_RADAR_LED
    help
        This option means support RADAR_STA Sample.

config SAMPLE_SUPPORT_RADAR_SOFTAP
    bool
    prompt "Support RADAR_SOFTAP Sample."
    default n
    depends on SAMPLE_SUPPORT_RADAR_LED
    help
        This option means support RADAR_SOFTAP Sample.